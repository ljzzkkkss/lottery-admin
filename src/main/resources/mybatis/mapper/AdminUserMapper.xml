<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ljzzkkkss.lottery.admin.mapper.AdminUserMapper" >

    <select id="findByUserName" parameterType="String" resultType="com.ljzzkkkss.lottery.admin.model.AdminUser">
        SELECT `id`,`username`,`password` FROM `admin_user`
        WHERE
        `username` = #{username}
    </select>

</mapper>